#include <stdio.h>

int main() {

    

    int matriz[3][3], soma = 0;

    printf("Digite os elementos da matriz 3x3:\n");

    for (int i = 0; i < 3; i++) {

        for (int j = 0; j < 3; j++) {

            scanf("%d", &matriz[i][j]);

            if (i == j) {

                soma += matriz[i][j];

            }

        }

    }

    printf("Soma dos elementos da diagonal principal: %d\n", soma);

    return 0;

}

