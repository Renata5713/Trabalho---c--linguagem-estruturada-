#include <stdio.h>

int main() {

    int matriz[3][3], soma = 0;

    printf("Digite os elementos da matriz 3x3:\n");

    for (int i = 0; i < 3; i++) {

        for (int j = 0; j < 3; j++) {

            scanf("%d", &matriz[i][j]);

            soma += matriz[i][j];

        }

    }

    float media = soma / 9.0; // 9 elementos em uma matriz 3x3

    printf("MÃ©dia dos elementos da matriz: %.2f\n", media);

    return 0;

}

