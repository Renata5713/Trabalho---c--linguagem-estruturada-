#include <stdio.h>

int main() {

    int A, B, C;

    printf("Digite os valores A, B e C: ");

    scanf("%d %d %d", &A, &B, &C);

    if (A + B < C) {

        printf("A + B é menor que C\n");

    } else if (A + B > C) {

        printf("A + B é maior que C\n");

    } else {

        printf("A + B é igual a C\n");

    }

    return 0;

}

